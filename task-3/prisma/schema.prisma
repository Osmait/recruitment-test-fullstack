// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id            Int    @id @default(autoincrement())
  Product_name  String
  price         Int
  description   String
  Product_Image String
}

model User {
  id       Int    @id @default(autoincrement())
  email    String
  password String
}
